{
  "name": "react-navigation-plane",
  "version": "1.5.1",
  "description": "React navigation infrastructure. Multiple stacks and advanced features.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "build:inc": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biels/react-navigation-plane.git"
  },
  "keywords": [
    "react",
    "navigation",
    "component"
  ],
  "author": "biels",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/biels/react-navigation-plane/issues"
  },
  "homepage": "https://github.com/biels/react-navigation-plane#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.5.0",
    "lodash": "^4.17.10",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "regenerator-runtime": "^0.12.1",
    "styled-components": "^3.4.2",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1",
    "typescript-babel-jest": "^1.0.5"
  },
  "dependencies": {
    "react-stateful-switch": "0.2.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-entity-plane": "^0.2.2",
    "styled-components": "^3.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "rootDir": "src",
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>setupTests.ts",
    "globals": {
      "ts-jest": {
        "skipBabel": false,
        "enableTsDiagnostics": false,
        "useBabelrc": true
      }
    }
  }
}
